package com.cmc.repaso.entidades;

public class Estudiante {
	public String nombre;
	private double nota;
	public String resultado;
	
	public Estudiante(String nombre) {
		//super();
		this.nombre = nombre;
	}
	
	public void notaCalificar(double nota) {
		if(this.nota >= 8) {
			 resultado = "A";
		}else {
			 resultado = "F";
		}
	}
}
 