package com.cmc.repaso.entidades;

public class Estudiante {
	private String nombre;
	private double nota;
	//private String resultado;
	
	public Estudiante(String nombre) {
		//super();
		this.nombre = nombre;
	}
	
	public void notaCalificar() {
		if(nota >= 8) {
			 System.out.println("A");
		}else {
			 System.out.println("A");
		}
	}
	
	
}
